    "@vendure/email-plugin": "2.1.4",
    "@vendure/payments-plugin": "^2.1.5",
    "@vendure/ui-devkit": "^2.1.5",
    "dotenv": "16.3.1",
    "mysql": "^2.18.1",
    "path": "^0.12.7",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@graphql-codegen/typescript": "4.0.6",
    "concurrently": "8.2.2",
    "ts-node": "10.9.2"
